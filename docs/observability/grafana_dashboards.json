{
  "dashboards": [
    {
      "dashboard": {
        "id": null,
        "title": "Valor IVX - SLOs and Service Level Objectives",
        "tags": ["valor-ivx", "slos", "monitoring"],
        "style": "dark",
        "timezone": "browser",
        "panels": [
          {
            "id": 1,
            "title": "API Availability SLO",
            "type": "stat",
            "targets": [
              {
                "expr": "rate(http_requests_total{status=~\"2..|3..\"}[5m]) / rate(http_requests_total[5m])",
                "legendFormat": "Availability"
              }
            ],
            "fieldConfig": {
              "defaults": {
                "color": {
                  "mode": "thresholds"
                },
                "thresholds": {
                  "steps": [
                    {"color": "red", "value": null},
                    {"color": "yellow", "value": 0.99},
                    {"color": "green", "value": 0.999}
                  ]
                },
                "unit": "percentunit"
              }
            },
            "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
          },
          {
            "id": 2,
            "title": "API Latency P95",
            "type": "stat",
            "targets": [
              {
                "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
                "legendFormat": "P95 Latency"
              }
            ],
            "fieldConfig": {
              "defaults": {
                "color": {
                  "mode": "thresholds"
                },
                "thresholds": {
                  "steps": [
                    {"color": "green", "value": null},
                    {"color": "yellow", "value": 0.1},
                    {"color": "red", "value": 0.2}
                  ]
                },
                "unit": "s"
              }
            },
            "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
          },
          {
            "id": 3,
            "title": "Financial Calculation Accuracy",
            "type": "stat",
            "targets": [
              {
                "expr": "rate(financial_calculations_total{status=\"success\"}[1h]) / rate(financial_calculations_total[1h])",
                "legendFormat": "Accuracy"
              }
            ],
            "fieldConfig": {
              "defaults": {
                "color": {
                  "mode": "thresholds"
                },
                "thresholds": {
                  "steps": [
                    {"color": "red", "value": null},
                    {"color": "yellow", "value": 0.99},
                    {"color": "green", "value": 0.999}
                  ]
                },
                "unit": "percentunit"
              }
            },
            "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
          },
          {
            "id": 4,
            "title": "User Session Success Rate",
            "type": "stat",
            "targets": [
              {
                "expr": "rate(active_users[1h]) / (rate(active_users[1h]) + rate(errors_total{error_type=\"session\"}[1h]))",
                "legendFormat": "Success Rate"
              }
            ],
            "fieldConfig": {
              "defaults": {
                "color": {
                  "mode": "thresholds"
                },
                "thresholds": {
                  "steps": [
                    {"color": "red", "value": null},
                    {"color": "yellow", "value": 0.99},
                    {"color": "green", "value": 0.995}
                  ]
                },
                "unit": "percentunit"
              }
            },
            "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
          },
          {
            "id": 5,
            "title": "SLO Compliance Overview",
            "type": "table",
            "targets": [
              {
                "expr": "vector(1)",
                "legendFormat": "API Availability",
                "format": "table"
              }
            ],
            "transformations": [
              {
                "id": "organize",
                "options": {
                  "excludeByName": {},
                  "indexByName": {},
                  "renameByName": {
                    "Time": "Metric",
                    "Value": "Compliance"
                  }
                }
              }
            ],
            "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
          }
        ],
        "time": {
          "from": "now-1h",
          "to": "now"
        },
        "refresh": "30s"
      }
    },
    {
      "dashboard": {
        "id": null,
        "title": "Valor IVX - System Performance and Metrics",
        "tags": ["valor-ivx", "performance", "metrics"],
        "style": "dark",
        "timezone": "browser",
        "panels": [
          {
            "id": 1,
            "title": "HTTP Request Rate",
            "type": "graph",
            "targets": [
              {
                "expr": "rate(http_requests_total[5m])",
                "legendFormat": "{{method}} {{endpoint}}"
              }
            ],
            "yAxes": [
              {
                "label": "Requests/sec",
                "min": 0
              }
            ],
            "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
          },
          {
            "id": 2,
            "title": "HTTP Response Time Distribution",
            "type": "heatmap",
            "targets": [
              {
                "expr": "rate(http_request_duration_seconds_bucket[5m])",
                "legendFormat": "{{le}}"
              }
            ],
            "yAxis": {
              "label": "Response Time (seconds)"
            },
            "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
          },
          {
            "id": 3,
            "title": "Financial Calculations Rate",
            "type": "graph",
            "targets": [
              {
                "expr": "rate(financial_calculations_total[5m])",
                "legendFormat": "{{calculation_type}} - {{status}}"
              }
            ],
            "yAxes": [
              {
                "label": "Calculations/sec",
                "min": 0
              }
            ],
            "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
          },
          {
            "id": 4,
            "title": "Financial Calculation Duration",
            "type": "graph",
            "targets": [
              {
                "expr": "rate(financial_calculation_duration_seconds_sum[5m]) / rate(financial_calculation_duration_seconds_count[5m])",
                "legendFormat": "{{calculation_type}}"
              }
            ],
            "yAxes": [
              {
                "label": "Duration (seconds)",
                "min": 0
              }
            ],
            "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
          },
          {
            "id": 5,
            "title": "Active Users",
            "type": "stat",
            "targets": [
              {
                "expr": "active_users",
                "legendFormat": "{{tenant}}"
              }
            ],
            "fieldConfig": {
              "defaults": {
                "color": {
                  "mode": "palette-classic"
                },
                "unit": "short"
              }
            },
            "gridPos": {"h": 8, "w": 6, "x": 0, "y": 16}
          },
          {
            "id": 6,
            "title": "WebSocket Connections",
            "type": "stat",
            "targets": [
              {
                "expr": "websocket_connections",
                "legendFormat": "{{tenant}}"
              }
            ],
            "fieldConfig": {
              "defaults": {
                "color": {
                  "mode": "palette-classic"
                },
                "unit": "short"
              }
            },
            "gridPos": {"h": 8, "w": 6, "x": 6, "y": 16}
          },
          {
            "id": 7,
            "title": "Error Rate",
            "type": "graph",
            "targets": [
              {
                "expr": "rate(errors_total[5m])",
                "legendFormat": "{{error_type}} - {{endpoint}}"
              }
            ],
            "yAxes": [
              {
                "label": "Errors/sec",
                "min": 0
              }
            ],
            "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
          }
        ],
        "time": {
          "from": "now-1h",
          "to": "now"
        },
        "refresh": "30s"
      }
    },
    {
      "dashboard": {
        "id": null,
        "title": "Valor IVX - System Resources and Health",
        "tags": ["valor-ivx", "system", "health"],
        "style": "dark",
        "timezone": "browser",
        "panels": [
          {
            "id": 1,
            "title": "CPU Usage",
            "type": "graph",
            "targets": [
              {
                "expr": "100 - (avg by (instance) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
                "legendFormat": "CPU Usage %"
              }
            ],
            "yAxes": [
              {
                "label": "CPU %",
                "min": 0,
                "max": 100
              }
            ],
            "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
          },
          {
            "id": 2,
            "title": "Memory Usage",
            "type": "graph",
            "targets": [
              {
                "expr": "(node_memory_MemTotal_bytes - node_memory_MemAvailable_bytes) / node_memory_MemTotal_bytes * 100",
                "legendFormat": "Memory Usage %"
              }
            ],
            "yAxes": [
              {
                "label": "Memory %",
                "min": 0,
                "max": 100
              }
            ],
            "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
          },
          {
            "id": 3,
            "title": "Disk Usage",
            "type": "graph",
            "targets": [
              {
                "expr": "(node_filesystem_size_bytes - node_filesystem_free_bytes) / node_filesystem_size_bytes * 100",
                "legendFormat": "Disk Usage %"
              }
            ],
            "yAxes": [
              {
                "label": "Disk %",
                "min": 0,
                "max": 100
              }
            ],
            "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
          },
          {
            "id": 4,
            "title": "Network I/O",
            "type": "graph",
            "targets": [
              {
                "expr": "rate(node_network_receive_bytes_total[5m])",
                "legendFormat": "Receive {{device}}"
              },
              {
                "expr": "rate(node_network_transmit_bytes_total[5m])",
                "legendFormat": "Transmit {{device}}"
              }
            ],
            "yAxes": [
              {
                "label": "Bytes/sec",
                "min": 0
              }
            ],
            "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
          },
          {
            "id": 5,
            "title": "Cache Hit Ratio",
            "type": "graph",
            "targets": [
              {
                "expr": "rate(cache_hits_total[5m]) / (rate(cache_hits_total[5m]) + rate(cache_misses_total[5m]))",
                "legendFormat": "{{cache_type}}"
              }
            ],
            "yAxes": [
              {
                "label": "Hit Ratio",
                "min": 0,
                "max": 1
              }
            ],
            "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
          },
          {
            "id": 6,
            "title": "System Health Status",
            "type": "stat",
            "targets": [
              {
                "expr": "up",
                "legendFormat": "Service Status"
              }
            ],
            "fieldConfig": {
              "defaults": {
                "color": {
                  "mode": "thresholds"
                },
                "thresholds": {
                  "steps": [
                    {"color": "red", "value": null},
                    {"color": "green", "value": 1}
                  ]
                },
                "mappings": [
                  {
                    "options": {
                      "0": {"text": "Down", "color": "red"},
                      "1": {"text": "Up", "color": "green"}
                    },
                    "type": "value"
                  }
                ]
              }
            },
            "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
          }
        ],
        "time": {
          "from": "now-1h",
          "to": "now"
        },
        "refresh": "30s"
      }
    },
    {
      "dashboard": {
        "id": null,
        "title": "Valor IVX - Business Metrics and Analytics",
        "tags": ["valor-ivx", "business", "analytics"],
        "style": "dark",
        "timezone": "browser",
        "panels": [
          {
            "id": 1,
            "title": "DCF Calculations by Type",
            "type": "piechart",
            "targets": [
              {
                "expr": "sum by (calculation_type) (financial_calculations_total{calculation_type=~\".*dcf.*\"})",
                "legendFormat": "{{calculation_type}}"
              }
            ],
            "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
          },
          {
            "id": 2,
            "title": "LBO Analysis Usage",
            "type": "graph",
            "targets": [
              {
                "expr": "rate(financial_calculations_total{calculation_type=~\".*lbo.*\"}[5m])",
                "legendFormat": "LBO Calculations/sec"
              }
            ],
            "yAxes": [
              {
                "label": "Calculations/sec",
                "min": 0
              }
            ],
            "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
          },
          {
            "id": 3,
            "title": "M&A Analysis Performance",
            "type": "graph",
            "targets": [
              {
                "expr": "rate(financial_calculation_duration_seconds_sum{calculation_type=~\".*ma.*\"}[5m]) / rate(financial_calculation_duration_seconds_count{calculation_type=~\".*ma.*\"}[5m])",
                "legendFormat": "M&A Calculation Duration"
              }
            ],
            "yAxes": [
              {
                "label": "Duration (seconds)",
                "min": 0
              }
            ],
            "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
          },
          {
            "id": 4,
            "title": "User Activity by Tenant",
            "type": "bargraph",
            "targets": [
              {
                "expr": "active_users",
                "legendFormat": "{{tenant}}"
              }
            ],
            "yAxes": [
              {
                "label": "Active Users",
                "min": 0
              }
            ],
            "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
          },
          {
            "id": 5,
            "title": "Collaboration Activity",
            "type": "graph",
            "targets": [
              {
                "expr": "websocket_connections",
                "legendFormat": "{{tenant}}"
              }
            ],
            "yAxes": [
              {
                "label": "WebSocket Connections",
                "min": 0
              }
            ],
            "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
          },
          {
            "id": 6,
            "title": "Error Distribution by Type",
            "type": "piechart",
            "targets": [
              {
                "expr": "sum by (error_type) (errors_total)",
                "legendFormat": "{{error_type}}"
              }
            ],
            "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
          }
        ],
        "time": {
          "from": "now-24h",
          "to": "now"
        },
        "refresh": "60s"
      }
    }
  ]
}
